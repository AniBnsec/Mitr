{
  "name": "MoodLog",
  "type": "object",
  "properties": {
    "mood": {
      "type": "string",
      "enum": [
        "Ecstatic",
        "Happy",
        "Neutral",
        "Sad",
        "Angry"
      ],
      "description": "The user's logged mood for the day"
    },
    "log_date": {
      "type": "string",
      "format": "date",
      "description": "The date of the mood log"
    },
    "user_id": {
      "type": "string",
      "description": "ID of the anonymous user"
    }
  },
  "required": [
    "mood",
    "log_date",
    "user_id"
  ]
}